{
  "name": "@kyso-io/kyso-store",
  "version": "1.1.17",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "lint": "eslint --ignore-path .eslintignore --fix --ext .js,.ts .",
    "tscBuild": "tsc --build",
    "build": "npm run clean && npm run tscBuild",
    "check-types": "tsc --noEmit --pretty",
    "prepare": "is-ci || husky install && npm run build",
    "dev": "tsc --watch",
    "start": "tsc",
    "test": "jest --coverage ; npm run cucumber",
    "cucumber": "./node_modules/.bin/cucumber-js -p default",
    "cucumber:local": "KYSO_API=http://localhost:4000/api/v1 npm run cucumber",
    "cucumber:local:tags": "KYSO_API=http://localhost:4000/api/v1 ./node_modules/.bin/cucumber-js -p default --tags @onlyonce",
    "test:verbose": "KYSO_CLI_VERBOSE=true jest --coverage",
    "prettier": "prettier --write \"./src/**/*.{ts,js}\""
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.kyso.io/kyso-io/libs/kyso-store.git"
  },
  "publishConfig": {
    "registry": "https://gitlab.kyso.io/api/v4/projects/9/packages/npm/"
  },
  "peerDependencies": {
    "@kyso-io/kyso-model": "1.2.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@cucumber/cucumber": "^8.7.0",
    "@cucumber/pretty-formatter": "^1.0.0",
    "@types/adm-zip": "^0.4.34",
    "@types/chai": "^4.3.3",
    "@types/cucumber": "^7.0.0",
    "@types/jest": "^28.1.6",
    "@types/node": "^17.0.8",
    "@types/sha256-file": "^1.0.1",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^8.0.0",
    "is-ci": "^3.0.1",
    "jest": "^28.1.3",
    "jest-html-reporters": "^3.0.10",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.7.1",
    "adm-zip": "^0.5.9",
    "axios": "^0.24.0",
    "dotenv": "^11.0.0",
    "form-data": "^4.0.0",
    "jszip": "^3.7.1",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.1",
    "original-fs": "^1.1.0",
    "reflect-metadata": "^0.1.13",
    "sha256-file": "^1.0.0",
    "slash": "^3.0.0",
    "slugify": "^1.6.5",
    "uuid": "^8.3.2"
  }
}
